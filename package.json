{
  "name": "@mo-id/pagination",
  "repository": "git://github.com/mo-id/moid-pagination.git",
  "author": "moid",
  "version": "0.1.3",
  "license": "MIT",
  "main": "__dist/index.js",
  "module": "__dist/esm/index.js",
  "typings": "__dist/index.d.ts",
  "files": [
    "__dist",
    "__src",
    "react"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && sh ./scripts/postbuild.sh",
    "compile": "tsc",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepack": "sh ./scripts/prepack.sh",
    "postpack": "sh ./scripts/postpack.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/moid-pagination.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moid-pagination.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@nestjs/common": ">=8",
    "@nestjs/config": ">=1",
    "@nestjs/core": ">=8",
    "react": ">=16"
  },
  "devDependencies": {
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
}
